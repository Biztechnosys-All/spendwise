@page "{packageName}"
@model Spendwise_WebApp.Pages.PackageModel
@{
    Layout = "~/Pages/Shared/_InnerPage_Layout.cshtml";
}

<div class="bg-overlay packages-banner-trust">
    <div class="col-five bg-image-packages gradient--green">
        <div class="container-inner">
            <div class="page-banner left-col-sp-heading">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-8 sp-heading">
                        <h1 class="mr-top-20-imp">@Model.Package.PackageName Package</h1>
                        <h2 class="fs-thirty-one fc-white fw-three lh-zero">@Html.DisplayFor(model => model.Package.ShortDescription)</h2>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-4">
                        <style>
                            .left-col-sp-heading.redux .banner-google__box {
                                margin-top: 0;
                            }

                            .banner-google__box {
                                display: flex !important;
                                flex-direction: column;
                                align-items: center;
                                justify-content: center;
                                position: relative;
                                background: white;
                                font-size: 18px;
                                font-size: 1.125rem;
                                text-align: center;
                                width: 250px;
                                max-width: 300px;
                                height: 150px;
                                padding: .8rem;
                                color: #2f2f2f;
                                margin: 0;
                                margin-top: 25px;
                                box-shadow: 0 5px 5px rgba(0, 0, 0, 0.25);
                            }

                            .banner-google__box-wrapper {
                                width: 100%;
                                margin: 0;
                                padding: 0;
                                display: flex;
                                align-items: flex-end;
                                justify-content: space-evenly;
                                overflow: hidden;
                            }

                            .banner-google__box:last-child {
                                margin-right: 0;
                            }

                            .banner-google__box svg {
                                display: inline-block;
                                vertical-align: top;
                                height: 1.5rem;
                                width: 100%;
                            }

                            .banner-google__box-title {
                                font-size: 18px;
                                font-size: 1.125rem;
                                color: white;
                                font-family: Myriad Pro Light Semi Condensed, sans-serif;
                                font-weight: 700;
                                margin-top: 0;
                            }

                            .banner-google__box__bottom {
                                width: 100%;
                                position: absolute;
                                color: white;
                                left: 0;
                                bottom: 15px;
                                border-top: 1px solid #ccc;
                                font-family: Arial, Helvetica, sans-serif;
                                font-size: 18px;
                                font-size: 1.125rem;
                            }

                            .banner-google__box--google-heading {
                                font-size: 24px;
                                font-size: 1.25rem;
                                display: inline-flex;
                                align-items: center;
                                font-family: MyriadW08-SemiCondensed !important;
                                font-weight: 600;
                                color: #3b3b3b;
                                margin-bottom: 0.5rem;
                            }

                            .banner-google__box--google-review-title {
                                margin-top: 4px;
                            }

                            .banner-google__box--google-logo {
                                height: 30px;
                                border: 1px solid #ccc;
                                border-radius: 50%;
                                margin-right: 0.75rem;
                            }

                            .banner-google__box--google-title {
                                font-size: 25px;
                                font-size: 1.5625rem;
                                font-weight: 400;
                                font-family: Myriad Pro Light Semi Condensed, sans-serif;
                            }

                            .banner-google__box--google-description {
                                font-size: 17px;
                                font-family: Myriad Pro Semi Condensed, sans-serif;
                                color: #3b3b3b;
                                width: 100%;
                            }

                            .banner-google__box--google-star {
                                margin-bottom: 5px;
                                height: 1rem;
                                width: 100%;
                            }

                            .banner-google__box--google-star-fill {
                                fill: #f0ac00;
                            }

                            .banner-google__box--google-star-fill-partial {
                                fill: url(#googleGradient);
                            }

                            .banner-google__box--google-star-fill-empty {
                                fill: none;
                            }

                            .banner-google__box--google-reviews {
                                font-size: 18px;
                                color: black;
                                font-family: Myriad Pro Light Semi Condensed, sans-serif;
                            }

                        </style>


                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
